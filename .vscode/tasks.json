{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "AtriumHub: Dev Server",
      "type": "npm",
      "script": "dev",
      "path": ".",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "isBackground": true,
      "presentation": {
        "reveal": "always",
        "panel": "dedicated",
        "clear": false
      },
      "problemMatcher": [
        {
          "owner": "nextjs",
          "pattern": [
            {
              "regexp": "^(.*)$",
              "file": 1,
              "location": 2,
              "message": 0
            }
          ],
          "background": {
            "activeOnStart": true,
            "beginsPattern": "Starting|â–² Next\\.js",
            "endsPattern": "Ready in"
          }
        }
      ]
    }
  ]
}
